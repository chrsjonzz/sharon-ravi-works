{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist",
        "buildCommand": "npm install && npm run build"
      }
    }
  ],
  "routes": [
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "installCommand": "npm install --legacy-peer-deps",
  "framework": "vite",
  "buildCommand": "npm install && npm run build",
  "devCommand": "vite",
  "outputDirectory": "dist",
  "regions": ["iad1"],
  "nodeVersion": "18.x"
}
